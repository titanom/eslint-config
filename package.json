{
  "name": "@titanom/eslint-config",
  "version": "1.0.0-alpha.1",
  "description": "Default ESLint config for projects at Titanom Technologies",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/titanom/eslint-config.git"
  },
  "author": "Jannis Morgenstern <jannis.n.morgenstern@gmail.com> (https://github.com/Jannis-Morgenstern)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/titanom/eslint-config/issues"
  },
  "homepage": "https://github.com/titanom/eslint-config#readme",
  "scripts": {
    "format": "prettier --write .",
    "build": "tsup && fix-tsup-cjs"
  },
  "exports": {
    "./javascript": {
      "require": "./dist/js/recommended.js",
      "import": "./dist/js/recommended.mjs"
    },
    "./typescript": {
      "require": "./dist/ts/recommended.js",
      "import": "./dist/ts/recommended.mjs"
    }
  },
  "devDependencies": {
    "@titanom/prettier-config": "^1.0.0",
    "eslint": "^8.47.0",
    "fix-tsup-cjs": "^1.2.0",
    "prettier": "^3.0.0",
    "tsup": "^7.2.0"
  },
  "dependencies": {
    "@babel/core": "^7.17.12",
    "@babel/eslint-parser": "^7.17.0",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "confusing-browser-globals": "^1.0.11",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-pii": "^1.0.2",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unicorn": "^42.0.0"
  },
  "peerDependencies": {
    "eslint": "^8.x.x",
    "react": "^18.x.x",
    "typescript": "^4.x.x"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  }
}
